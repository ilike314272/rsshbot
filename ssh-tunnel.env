# ~/.ssh-tunnel.env

# GitHub repo settings
REPO_DIR="$HOME/RSSHinfo"
GITHUB_REPO="git@github.com:ilike314272/RSSHinfo.git"
GIT_BRANCH="main"

# EC2 settings
EC2_USER="ubuntu"
EC2_HOST="2600:1f16:371:71f:f92c:2658:2fd2:7f5e"
SSH_KEY="$HOME/.ssh/id_ed25519_rssh_ec2"

# Local and remote ports (customize per machine)
LOCAL_PORT=22
REMOTE_PORT=2201

# Add this if not already there
NETWORK_INTERFACE="eth0"       # or enp0s3, etc.
IPV6_FILE_PATH="$REPO_DIR/public_ipv6_(hostname)"
